<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".StartupF">


    <Button
        android:id="@+id/authentication_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Authentication"
        android:layout_marginTop="50dp"
        android:layout_centerHorizontal="true"/>

    <Button
        android:id="@+id/transaction_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Transaction"
        android:layout_below="@id/authentication_button"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp"
         />

    <Button
        android:id="@+id/pharmacy_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pharmacies"
        android:layout_below="@id/transaction_button"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp"

        />
        app:layout_constraintBottom_toTopOf="@+id/searchDoc_button"
        app:layout_constraintStart_toStartOf="@+id/transaction_button"
        app:layout_constraintTop_toBottomOf="@+id/transaction_button" />

    <Button
        android:id="@+id/button_MedicalRecords"
        android:id="@+id/searchDoc_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Medical Records"
        android:layout_below="@id/pharmacy_button"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp"

        />
        android:layout_marginEnd="13dp"
        android:layout_marginBottom="3dp"
        android:text="SearchDoc"
        app:layout_constraintBottom_toTopOf="@+id/extra2_button"
        app:layout_constraintEnd_toEndOf="@+id/pharmacy_button"
        app:layout_constraintTop_toBottomOf="@+id/pharmacy_button" />

    <Button
        android:id="@+id/extra2_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Extra 2"
        android:layout_below="@id/button_MedicalRecords"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp"
        />
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searchDoc_button" />

</RelativeLayout>