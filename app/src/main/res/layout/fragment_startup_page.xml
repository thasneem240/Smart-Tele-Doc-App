<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".StartupF">


    <Button
        android:id="@+id/authentication_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="Authentication"
        app:layout_constraintBottom_toTopOf="@+id/transaction_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/transaction_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="2dp"
        android:text="Transaction"
        app:layout_constraintBottom_toTopOf="@+id/pharmacy_button"
        app:layout_constraintEnd_toEndOf="@+id/authentication_button"
        app:layout_constraintTop_toBottomOf="@+id/authentication_button" />

    <Button
        android:id="@+id/pharmacy_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="2dp"
        android:text="Pharmacies"
        app:layout_constraintBottom_toTopOf="@+id/button_MedicalRecords"
        app:layout_constraintStart_toStartOf="@+id/transaction_button"
        app:layout_constraintTop_toBottomOf="@+id/transaction_button" />

    <Button
        android:id="@+id/button_MedicalRecords"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Medical Records"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="403dp" />

    <Button
        android:id="@+id/extra2_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Extra 2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_MedicalRecords" />

</androidx.constraintlayout.widget.ConstraintLayout>