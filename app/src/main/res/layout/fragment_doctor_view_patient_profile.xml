<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".medical_records_prescriptions.DetailedPrescription">

    <TextView
        android:id="@+id/medical_rec2_dpp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="102dp"
        android:layout_marginBottom="15dp"
        android:text="Patient details"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/button_back_dpp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="63dp"
        android:layout_marginBottom="57dp"
        android:src="@drawable/baseline_keyboard_arrow_left_24"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout3"
        android:layout_width="0dp"
        android:layout_height="493dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="19dp"
        android:background="@drawable/blue_color"
        app:layout_constraintBottom_toTopOf="@+id/button_goBack"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/medical_rec2_dpp">


        <TextView
            android:id="@+id/textView27_dpp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="28dp"
            android:text="First Name"
            android:textColor="@android:color/black"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView30_dpp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="33dp"
            android:text="Last Name"
            android:textColor="@color/black"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_firstName_dpp" />

        <TextView
            android:id="@+id/textView_lastName_dpp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="last_name"
            app:layout_constraintStart_toStartOf="@+id/textView30_dpp"
            app:layout_constraintTop_toBottomOf="@+id/textView30_dpp" />

        <TextView
            android:id="@+id/textView_height_dpp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="height"
            app:layout_constraintStart_toStartOf="@+id/textView36_dpp"
            app:layout_constraintTop_toBottomOf="@+id/textView36_dpp" />

        <TextView
            android:id="@+id/textView36_dpp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="33dp"
            android:text="Height"
            android:textColor="@color/black"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_gender_dpp" />

        <TextView
            android:id="@+id/textView34_dpp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="33dp"
            android:layout_marginEnd="278dp"
            android:text="Gender"
            android:textColor="@color/black"
            app:layout_constraintBottom_toTopOf="@+id/textView_gender_dpp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_dob_dpp" />

        <TextView
            android:id="@+id/textView_gender_dpp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="279dp"
            android:layout_marginBottom="34dp"
            android:text="gender"
            app:layout_constraintBottom_toTopOf="@+id/textView36_dpp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView34_dpp" />

        <TextView
            android:id="@+id/textView32_dpp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="34dp"
            android:layout_marginEnd="245dp"
            android:text="Date of birth"
            android:textColor="@color/black"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_lastName_dpp" />

        <TextView
            android:id="@+id/textView_dob_dpp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="1dp"
            android:layout_marginBottom="33dp"
            android:text="dob"
            app:layout_constraintBottom_toTopOf="@+id/textView34_dpp"
            app:layout_constraintStart_toStartOf="@+id/textView32_dpp"
            app:layout_constraintTop_toBottomOf="@+id/textView32_dpp"
            app:layout_constraintVertical_bias="0.56" />

        <TextView
            android:id="@+id/textView_firstName_dpp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="first_name"
            app:layout_constraintStart_toStartOf="@+id/textView27_dpp"
            app:layout_constraintTop_toBottomOf="@+id/textView27_dpp" />

        <TextView
            android:id="@+id/textView33_dpp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="34dp"
            android:layout_marginEnd="85dp"
            android:text="Weight"
            android:textColor="@color/black"
            app:layout_constraintEnd_toStartOf="@+id/progressBar_dpp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_height_dpp" />

        <TextView
            android:id="@+id/textView_weight_dpp"
            android:layout_width="42dp"
            android:layout_height="19dp"
            android:layout_marginStart="16dp"
            android:text="weight"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView33_dpp"
            app:layout_constraintVertical_bias="0.0" />

        <ProgressBar
            android:id="@+id/progressBar_dpp"
            style="?android:attr/progressBarStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="388dp"
            android:layout_marginBottom="57dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/button_goBack"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_marginTop="1dp"
        android:layout_marginBottom="27dp"
        android:background="@drawable/rectangle_10"
        android:text="Go back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout3" />

</androidx.constraintlayout.widget.ConstraintLayout>