<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".interface_of_doctors.MainActivity4">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/docmenufragmentContainer"
        android:layout_width="407dp"
        android:layout_height="668dp"
        android:layout_marginBottom="64dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/homePageButton2"
        android:layout_width="28dp"
        android:layout_height="25dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="18dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/menu_home_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/docmenufragmentContainer"
        app:srcCompat="@drawable/menu_home_icon" />

    <Button
        android:id="@+id/chatBotButton2"
        android:layout_width="28dp"
        android:layout_height="25dp"
        android:layout_marginStart="59dp"
        android:layout_marginTop="18dp"
        android:background="@drawable/menu_message_icon"
        app:layout_constraintStart_toEndOf="@+id/homePageButton2"
        app:layout_constraintTop_toBottomOf="@+id/docmenufragmentContainer"
        app:srcCompat="@drawable/menu_message_icon" />

    <Button
        android:id="@+id/appointmentButton2"
        android:layout_width="28dp"
        android:layout_height="25dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="68dp"
        android:background="@drawable/menu_appointment_icon"
        app:layout_constraintEnd_toStartOf="@+id/userProfileButton2"
        app:layout_constraintTop_toBottomOf="@+id/docmenufragmentContainer"
        app:srcCompat="@drawable/menu_appointment_icon" />

    <Button
        android:id="@+id/userProfileButton2"
        android:layout_width="28dp"
        android:layout_height="25dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/menu_profile_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/docmenufragmentContainer"
        app:srcCompat="@drawable/menu_profile_icon" />
</androidx.constraintlayout.widget.ConstraintLayout>