<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".writing_prescriptions.WritingPrescriptionActivity">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragmentContainerPrescription"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <!--android:name="com.example.capstoneprojectgroup4.prescriptions.writing_prescriptions.CreatePrescriptionFragment"-->


    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/FragmentContainerView_SelectTheDrug"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/FragmentContainerView_Search" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/FragmentContainerView_Search"
        android:layout_width="0dp"
        android:layout_height="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <!--        android:name="com.example.capstoneprojectgroup4.prescriptions.writing_prescriptions.SearchWordByWord"-->
</androidx.constraintlayout.widget.ConstraintLayout>