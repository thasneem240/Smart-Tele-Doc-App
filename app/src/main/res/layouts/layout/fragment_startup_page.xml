<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="home.StartupF">


    <Button
        android:id="@+id/button_MedicalRecords"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/search_drugs_button"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="132dp"
        android:layout_marginBottom="49dp"
        android:text="Medical Records"
        app:layout_constraintBottom_toTopOf="@+id/searchDoc_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pharmacy_button" />

    <Button
        android:id="@+id/search_drugs_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/authentication_button"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="141dp"
        android:layout_marginBottom="47dp"
        android:text="Search Drugs"
        app:layout_constraintBottom_toTopOf="@+id/chatbot_button"
        app:layout_constraintStart_toStartOf="@+id/searchDoc_button"
        app:layout_constraintTop_toBottomOf="@+id/searchDoc_button" />

    <Button
        android:id="@+id/authentication_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/transaction_button"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="135dp"
        android:layout_marginBottom="26dp"
        android:text="Authentication"
        app:layout_constraintBottom_toTopOf="@+id/transaction_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.511"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/transaction_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/pharmacy_button"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="152dp"
        android:layout_marginBottom="26dp"
        android:text="Transaction"
        app:layout_constraintBottom_toTopOf="@+id/pharmacy_button"
        app:layout_constraintStart_toStartOf="@+id/authentication_button"
        app:layout_constraintTop_toBottomOf="@+id/authentication_button" />

    <Button
        android:id="@+id/pharmacy_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_above="@+id/searchDoc_button"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="158dp"
        android:layout_marginBottom="25dp"
        android:text="Pharmacies"
        app:layout_constraintBottom_toTopOf="@+id/button_MedicalRecords"
        app:layout_constraintStart_toStartOf="@+id/button_MedicalRecords"
        app:layout_constraintTop_toBottomOf="@+id/transaction_button" />

    <Button
        android:id="@+id/searchDoc_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/chatbot_button"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="143dp"
        android:layout_marginBottom="18dp"
        android:text="Search Doctors"
        app:layout_constraintBottom_toTopOf="@+id/search_drugs_button"
        app:layout_constraintEnd_toEndOf="@+id/button_MedicalRecords"
        app:layout_constraintTop_toBottomOf="@+id/button_MedicalRecords" />

    <Button
        android:id="@+id/chatbot_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/front_end_button"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="174dp"
        android:layout_marginBottom="21dp"
        android:text="Chatbot"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/search_drugs_button"
        app:layout_constraintTop_toBottomOf="@+id/search_drugs_button" />

    <Button
        android:id="@+id/front_end_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="161dp"
        android:layout_marginBottom="22dp"
        android:text="Front End"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.458"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.904" />

    <Button
        android:id="@+id/consultations"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button_MedicalRecords"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="46dp"
        android:layout_marginEnd="140dp"
        android:layout_marginBottom="40dp"
        android:text="Consultations"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.935" />

</RelativeLayout>



