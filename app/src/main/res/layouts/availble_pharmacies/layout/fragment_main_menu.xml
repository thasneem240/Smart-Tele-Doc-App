<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/good_mornin"
        android:layout_width="252dp"
        android:layout_height="27dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="83dp"
        android:layout_marginTop="74dp"
        android:gravity="top"
        android:text="Good Morning, Nethmi"
        android:textColor="#000000"
        android:textSize="22sp" />

    <TextView
        android:id="@+id/your_virtua"
        android:layout_width="284dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="23dp"
        android:layout_marginTop="130dp"
        android:gravity="top"
        android:lineSpacingExtra="3sp"
        android:text="Your Virtual Doctor Is \nAlways Ready to Listen!"
        android:textColor="#101623"
        android:textSize="16sp"
        android:translationY="-1.61sp" />

    <TextView
        android:id="@+id/how_are_you"
        android:layout_width="228dp"
        android:layout_height="27dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="195dp"
        android:gravity="top"
        android:text="How are you feeling today?"
        android:textColor="#000000"
        android:textSize="14sp" />


    <ImageView
        android:id="@+id/your_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/good_mornin"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="13dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="24dp"
        android:layout_toEndOf="@+id/how_are_you"
        android:src="@drawable/doctor_image" />

    <ImageView
        android:id="@+id/docSquare"
        android:layout_width="139.29dp"
        android:layout_height="120dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="38dp"
        android:layout_marginTop="256dp"
        android:src="@drawable/green_square" />


    <Button
        android:id="@+id/doctor_Button"
        android:layout_width="63dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="75dp"
        android:layout_marginTop="274dp"
        android:background="@drawable/layered_doc_button" />


    <Button
        android:id="@+id/pharma_Button"
        android:layout_width="63dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="276dp"
        android:layout_marginTop="271dp"
        android:background="@drawable/layered_pharmacy_button" />

    <Button
        android:id="@+id/emergencyButton"
        android:layout_width="60dp"
        android:layout_height="57dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="73dp"
        android:layout_marginTop="413dp"
        android:background="@drawable/layered_emergency_button" />

    <Button
        android:id="@+id/aiButton"
        android:layout_width="63dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="274dp"
        android:layout_marginTop="414dp"
        android:background="@drawable/layered_ai_button" />


    <Button
        android:id="@+id/records_Button"
        android:layout_width="63dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="270dp"
        android:layout_marginTop="562dp"
        android:background="@drawable/layered_records_button"
        android:contentDescription="@string/app_name" />

    <Button
        android:id="@+id/transactionButton"
        android:layout_width="63dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="73dp"
        android:layout_marginTop="563dp"
        android:background="@drawable/layered_transaction_icon"
        android:contentDescription="@string/app_name" />

    <TextView
        android:id="@+id/doctor"
        android:layout_width="151dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="31dp"
        android:layout_marginTop="344dp"
        android:gravity="center_horizontal|top"
        android:text="Doctor"
        android:textColor="#000000"
        android:textSize="16sp" />


    <ImageView
        android:id="@+id/pharmacySquare"
        android:layout_width="139.29dp"
        android:layout_height="120dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginLeft="234dp"
        android:layout_marginTop="254dp"
        android:layout_marginEnd="38dp"
        android:background="@drawable/yellow_square"
        android:contentDescription="Pharmacy Button" />

    <TextView
        android:id="@+id/pharmacytext"
        android:layout_width="151dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="231dp"
        android:layout_marginTop="341dp"
        android:gravity="center_horizontal|top"
        android:text="Pharmacy"
        android:textColor="#000000"
        android:textSize="16sp" />

    <ImageView
        android:id="@+id/emergencySquare"
        android:layout_width="139.29dp"
        android:layout_height="120dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="38dp"
        android:layout_marginTop="399dp"
        android:background="@drawable/blue_square" />


    <TextView
        android:id="@+id/emergency"
        android:layout_width="149dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="482dp"
        android:gravity="center_horizontal|top"
        android:text="Emergency"
        android:textColor="#000000"
        android:textSize="16sp" />


    <ImageView
        android:id="@+id/Ai_square"
        android:layout_width="139.29dp"
        android:layout_height="120dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginLeft="234dp"
        android:layout_marginTop="399dp"
        android:layout_marginEnd="38dp"
        android:background="@drawable/green_square" />

    <TextView
        android:id="@+id/ai_helper"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="236dp"
        android:layout_marginTop="487dp"
        android:gravity="center_horizontal|top"
        android:text="AI Helper"
        android:textColor="#000000"
        android:textSize="16sp" />

    <ImageView
        android:id="@+id/transaction_square"
        android:layout_width="139.29dp"
        android:layout_height="120dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="38dp"
        android:layout_marginTop="546dp"
        android:background="@drawable/yellow_square" />


    <TextView
        android:id="@+id/transaction"
        android:layout_width="151dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="635dp"
        android:gravity="center_horizontal|top"
        android:text="Transactions"
        android:textColor="#000000"
        android:textSize="16sp" />

    <ImageView
        android:id="@+id/records_square"
        android:layout_width="139.29dp"
        android:layout_height="120dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginLeft="234dp"
        android:layout_marginTop="545dp"
        android:layout_marginEnd="38dp"
        android:background="@drawable/blue_square" />

    <TextView
        android:id="@+id/records"
        android:layout_width="145dp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="235dp"
        android:layout_marginTop="631dp"
        android:gravity="center_horizontal|top"
        android:text="Records"
        android:textColor="#000000"
        android:textSize="16sp" />

</RelativeLayout>